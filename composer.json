{
    "name": "mstenta/farm_nfa",
    "description": "farmOS Deployment for National Forestry Authority of Uganda",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://farmOS.org",
    "support": {
        "docs": "https://farmOS.org/guide",
        "forum": "https://farmOS.discourse.group",
        "chat": "https://matrix.to/#/#farmOS:matrix.org"
    },
    "authors": [
        {
            "name": "Michael Stenta",
            "email": "mike@farmier.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/farmOS/farmOS.git"
        },
        {
            "type": "git",
            "url": "https://github.com/farmOS/farm_forest.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "cweagans/composer-patches": "^1.6",
        "drupal/core-composer-scaffold": "~9.1.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/farm_forest": "2.x",
        "farmos/farmos": "2.0.0-alpha2",
        "oomphinc/composer-installers-extender": "^2"
    },
    "require-dev": {
        "brianium/paratest": "^4",
        "drupal/core-dev": "~9.1.0",
        "phpspec/prophecy-phpunit": "^2"
    },
    "_comments": {
        "phpspec/prophecy-phpunit": "Required to fix ProphecyTrait not found, until Drupal core drops support for PHPUnit 8. See https://www.drupal.org/node/3176567"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "installer-types": ["npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/profiles/farm": ["farmos/farmos"],
            "web/modules/farm_forest": ["drupal/farm_forest"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/libraries/farmOS-map": ["npm-asset/farmos.org--farmos-map"],
            "web/modules/{$name}": ["type:drupal-module"],
            "web/themes/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "vendor/npm-asset/{$name}": ["type:npm-asset"]
        },
        "patches": {
            "drupal/core": {
                "Issue #3192365: Race Condition in 'public://simpletest' mkdir Call": "https://www.drupal.org/files/issues/2021-01-12/3192365-3.patch"
            },
            "drupal/entity_reference_revisions": {
                "Issue #2667748: Allow single scalar value in EntityReferenceRevisions data type plugin": "https://www.drupal.org/files/issues/2020-04-14/allow_scalar_values-2667748-D8-41.patch"
            }
        }
    }
}
