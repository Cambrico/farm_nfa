{
    "name": "mstenta/farm_nfa",
    "description": "farmOS Deployment for National Forestry Authority of Uganda",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://farmOS.org",
    "support": {
        "docs": "https://farmOS.org/guide",
        "forum": "https://farmOS.discourse.group",
        "chat": "https://matrix.to/#/#farmOS:matrix.org"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/farmOS/farmOS.git"
        },
        {
            "type": "git",
            "url": "https://github.com/farmOS/farm_forest.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "lucianocosta/jquery.mtz.monthpicker",
                "version": "master",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/lucianocosta/jquery.mtz.monthpicker.git",
                    "type": "git",
                    "reference":"master"

                }
            }
        }
    ],
    "require": {
        "cweagans/composer-patches": "^1.7",
        "drupal/config_devel": "^1.8",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/element_multiple": "1.x-dev",
        "drupal/entity_dialog_formatter": "^1.0@beta",
        "drupal/farm_forest": "2.x",
        "drupal/field_group": "^3.1",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/month_year": "^2.0",
        "drupal/select_or_other": "^4.0",
        "farmos/farmos": "2.0.0-alpha5",
        "lucianocosta/jquery.mtz.monthpicker": "master",
        "oomphinc/composer-installers-extender": "^2",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-types": ["npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/profiles/farm": ["farmos/farmos"],
            "web/modules/farm_forest": ["drupal/farm_forest"],
            "web/libraries/{$name}": ["type:drupal-library", "vendor:npm-asset"],
            "web/modules/{$name}": ["type:drupal-module"],
            "web/themes/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/entity_reference_revisions": {
                "Issue #2667748: Allow single scalar value in EntityReferenceRevisions data type plugin": "https://www.drupal.org/files/issues/2020-04-14/allow_scalar_values-2667748-D8-41.patch"
            },
            "drupal/select_or_other": {
                "Issue #3227558: AllowedValuesConstraintValidator::getValidChoices only checks for config fields and not base fields": "https://www.drupal.org/files/issues/2021-08-10/3227558-2.patch"
            },
            "drupal/gin": {
                "Issue #3214515-4: Status messages added using JS are not dismissable": "https://www.drupal.org/files/issues/2021-11-11/3214515-8.patch"
            },
            "drupal/core": {
                "Issue #1874838-26: Allow exposed blocks to use 'Link display' settings": "https://www.drupal.org/files/issues/2021-11-11/1874838-26.patch",
                "Issue #2985882-85: Workaround for Call to a member function in layout builder":"https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "web/profiles/farm/composer.libraries.json",
                "web/profiles/farm/composer.project.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true
        }
    }
}
